<h3>Current spreadsheets</h3>
  <ul>
    <% Spreadsheet.all.each do |spreadsheet| %>
    </li>
      <%= spreadsheet.google_key %>
      <a target="_new" href="https://docs.google.com/spreadsheet/pub?hl=en_US&hl=en_US&key=<%= spreadsheet.google_key %>&output=html">Google</a>
<!--  <a target="_new" href="https://s3.amazonaws.com/ ENV['AWS_BUCKET'] %>/ spreadsheet.google_key %>">S3</a> -->
        <% if !@updated.nil? and @updated.include?(spreadsheet) %>
          <span class="label label-success">Updated</span>
        <% end %>
    </li>
    <% end %> 
  </ul> 
  <form action="/process" method="post">
    <p><button type="submit" class="btn btn-submit">Sync all spreadsheets</button></p>
  </form>
</div>